{
  "private": true,
  "name": "@contentful/optimization",
  "description": "Contentful Optimization SDK Suite",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "build": "pnpm --filter @contentful/* build",
    "build:all": "pnpm -r build",
    "clean": "pnpm -r --parallel clean",
    "docs:generate": "typedoc",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --check --write",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "pnpm --filter @contentful/* test:unit",
    "test:unit:all": "pnpm -r test:unit",
    "typecheck": "pnpm -r typecheck",
    "version:node": "node -v",
    "version:pnpm": "pnpm -v"
  },
  "engines": {
    "node": ">=16.20.0"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "commitizen": "^4.3.1",
    "cz-vinyl": "^2.5.4",
    "eslint": "^9.29.0",
    "eslint-config-love": "^121.0.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.1",
    "typedoc": "^0.28.7",
    "typescript": "catalog:",
    "typescript-eslint": "^8.35.0",
    "vitest": "catalog:"
  }
}
