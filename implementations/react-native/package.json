{
  "name": "ReactNativeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "start": "react-native start",
    "start:clean": "kill-port 8081 && react-native start --reset-cache",
    "prestart": "kill-port 8081 || true",
    "test": "jest",
    "test:unit": "echo 'Unit tests skipped - this is a reference implementation app. E2E tests are run separately via Detox.'",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "e2e:ios": "pnpm run e2e:build:ios && pnpm run test:e2e:ios",
    "e2e:android": "pnpm run e2e:build:android && pnpm run test:e2e:android"
  },
  "dependencies": {
    "@contentful/optimization-react-native": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "react": "18.3.1",
    "react-native": "0.76.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@jest/globals": "^29.6.3",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.9",
    "@react-native/metro-config": "0.76.9",
    "@react-native/typescript-config": "0.76.9",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "detox": "^20.43.0",
    "jest": "^29.6.3",
    "jest-circus": "^30.2.0",
    "kill-port": "^2.0.1",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  }
}
