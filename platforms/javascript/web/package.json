{
  "name": "@contentful/optimization-web",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build:esm": "vite build -c vite.esm.config.ts",
    "build:umd": "vite build -c vite.umd.config.ts",
    "build": "pnpm clean && tsc -b ./tsconfig.build.json && pnpm build:esm && pnpm build:umd",
    "clean": "rimraf ./dist ./coverage tsconfig.tsbuildinfo",
    "dev": "vite -c vite.esm.config.ts --host",
    "test:unit": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contentful/optimization-core": "workspace:*",
    "@types/js-cookie": "3.0.6",
    "es-toolkit": "catalog:",
    "js-cookie": "3.0.5",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "catalog:",
    "happy-dom": "^20.0.2",
    "rimraf": "catalog:",
    "rollup-plugin-visualizer": "^6.0.3",
    "tslib": "catalog:",
    "typescript": "catalog:",
    "vite": "^7.1.11",
    "vitest": "catalog:",
    "vite-bundle-analyzer": "^1.2.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
