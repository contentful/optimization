{
  "name": "@contentful/optimization-node",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "pnpm clean; rslib build && build-tools emit-dual-dts ./dist",
    "clean": "rimraf ./.rslib ./dist ./coverage .tsbuildinfo",
    "dev": "tsx watch --env-file=.env ./server.ts",
    "test:unit": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contentful/optimization-core": "workspace:*",
    "es-toolkit": "catalog:"
  },
  "devDependencies": {
    "@contentful/rich-text-html-renderer": "catalog:",
    "@contentful/rich-text-types": "catalog:",
    "@microsoft/api-extractor": "catalog:",
    "@rslib/core": "catalog:",
    "@types/express": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "build-tools": "workspace:*",
    "contentful": "catalog:",
    "express": "catalog:",
    "express-rate-limit": "catalog:",
    "pug": "^3.0.3",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
