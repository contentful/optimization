{
  "name": "@contentful/optimization-node",
  "version": "0.0.0",
  "license": "MIT",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "clean": "rimraf ./dist ./coverage tsconfig.tsbuildinfo",
    "dev": "tsx watch --env-file=.env ./server.ts",
    "test:unit": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contentful/optimization-core": "workspace:*",
    "es-toolkit": "catalog:"
  },
  "devDependencies": {
    "@types/express": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@contentful/rich-text-html-renderer": "catalog:",
    "@contentful/rich-text-types": "catalog:",
    "contentful": "catalog:",
    "express": "catalog:",
    "express-rate-limit": "catalog:",
    "pug": "^3.0.3",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "tsx": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
