{
  "name": "@contentful/optimization-web-preview-panel",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:dist",
    "build:ci": "pnpm build:dist",
    "build:dist": "rslib build && build-tools emit-dual-dts ./dist",
    "build:rsdoctor": "RSDOCTOR=true rslib build",
    "clean": "rimraf ./.rsdoctor ./.rslib ./dist ./coverage tsconfig.tsbuildinfo",
    "dev": "rsbuild dev --host 0.0.0.0 -c rsbuild.config.ts",
    "test:unit": "echo \"TODO: Unit tests\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contentful/optimization-web": "workspace:*",
    "@lit/context": "^1.1.6",
    "contentful": "catalog:",
    "es-toolkit": "catalog:",
    "lit": "^3.3.2"
  },
  "devDependencies": {
    "@rsbuild/core": "catalog:",
    "@rslib/core": "catalog:",
    "build-tools": "workspace:*",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:"
  }
}
