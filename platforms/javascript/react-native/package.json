{
  "name": "@contentful/optimization-react-native",
  "version": "1.0.0",
  "license": "MIT",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "build": "pnpm clean; pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc -b ./tsconfig.cjs.json && pnpm rename:js:cjs",
    "build:esm": "tsc -b ./tsconfig.esm.json && pnpm rename:js:mjs",
    "rename:js:cjs": "find ./dist -type f -name \"*.js\" ! -name \"*.cjs\" -print0 | while IFS= read -r -d '' file; do mv \"$file\" \"${file%.js}.cjs\"; done && find ./dist -type f -name \"*.js.map\" -print0 | while IFS= read -r -d '' file; do mv \"$file\" \"${file%.js.map}.cjs.map\"; done",
    "rename:js:mjs": "find ./dist -type f -name \"*.js\" ! -name \"*.mjs\" -print0 | while IFS= read -r -d '' file; do mv \"$file\" \"${file%.js}.mjs\"; done && find ./dist -type f -name \"*.js.map\" -print0 | while IFS= read -r -d '' file; do mv \"$file\" \"${file%.js.map}.mjs.map\"; done",
    "clean": "rimraf ./dist ./coverage tsconfig.tsbuildinfo",
    "test:unit": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "dev:android": "react-native run-android --projectRoot dev --config dev/metro.config.js",
    "dev:ios": "react-native run-ios --projectRoot dev --config dev/metro.config.js",
    "dev:lint": "cd dev && eslint .",
    "dev:lint:fix": "cd dev && eslint . --fix",
    "dev:format": "cd dev && prettier . --check",
    "dev:format:fix": "cd dev && prettier . --write",
    "dev:start": "react-native start --projectRoot dev --config dev/metro.config.js",
    "dev:start:clean": "kill-port 8081 && react-native start --reset-cache --projectRoot dev --config dev/metro.config.js",
    "dev:prestart": "kill-port 8081 || true",
    "dev:serve:mocks": "pnpm --filter mocks serve",
    "dev:build": "cd dev && react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "dev:test": "cd dev && jest"
  },
  "dependencies": {
    "@contentful/optimization-core": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "es-iterator-helpers": "^1.2.1",
    "es-toolkit": "catalog:",
    "react": "^18.3.1",
    "react-native": "^0.76.9",
    "react-native-get-random-values": "^2.0.0",
    "react-native-uuid": "^2.0.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@jest/globals": "^29.6.3",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.9",
    "@react-native/metro-config": "0.76.9",
    "@react-native/typescript-config": "0.76.9",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^18.3.18",
    "@types/react-test-renderer": "^18.0.0",
    "@vitest/coverage-v8": "catalog:",
    "babel-jest": "^29.6.3",
    "contentful": "catalog:",
    "core-js": "^3.46.0",
    "jest": "^29.6.3",
    "jest-circus": "^30.2.0",
    "kill-port": "^2.0.1",
    "react-test-renderer": "18.3.1",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "engines": {
    "node": ">=18"
  }
}
