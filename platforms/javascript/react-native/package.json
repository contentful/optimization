{
  "name": "@contentful/optimization-react-native",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "pnpm clean; rslib build && build-tools emit-dual-dts ./dist",
    "clean": "rimraf ./.rslib ./dist ./coverage .tsbuildinfo",
    "test:unit": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "dev:android": "cd dev && react-native run-android",
    "dev:ios": "cd dev && react-native run-ios",
    "dev:start": "cd dev && react-native start",
    "dev:start:clean": "kill-port 8081 && cd dev && react-native start --reset-cache",
    "dev:test": "jest --config dev/jest.config.js",
    "dev:setup:ios": "./dev/scripts/run-dev-dashboard.sh --ios",
    "dev:setup:android": "./dev/scripts/run-dev-dashboard.sh --android",
    "dev:setup:servers": "./dev/scripts/run-dev-dashboard.sh --no-app"
  },
  "dependencies": {
    "@contentful/optimization-core": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-clipboard/clipboard": "^1.15.0",
    "es-iterator-helpers": "^1.2.1",
    "es-toolkit": "catalog:",
    "react": "^18.3.1",
    "react-native": "^0.76.9",
    "react-native-get-random-values": "^2.0.0",
    "react-native-uuid": "^2.0.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "mocks": "workspace:*",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@jest/globals": "^29.6.3",
    "@microsoft/api-extractor": "catalog:",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.9",
    "@react-native/metro-config": "0.76.9",
    "@react-native/typescript-config": "0.76.9",
    "@testing-library/react-native": "^13.3.3",
    "@types/node": "catalog:",
    "@types/react": "^18.3.18",
    "@types/react-test-renderer": "^18.0.0",
    "@rslib/core": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "babel-jest": "^29.6.3",
    "build-tools": "workspace:*",
    "contentful": "catalog:",
    "core-js": "^3.46.0",
    "dotenv": "catalog:",
    "jest": "^29.6.3",
    "jest-circus": "^30.2.0",
    "kill-port": "^2.0.1",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@react-native-community/netinfo": ">=9.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "peerDependenciesMeta": {
    "@react-native-community/netinfo": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
