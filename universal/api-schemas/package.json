{
  "name": "@contentful/optimization-api-schemas",
  "version": "0.0.0",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "pnpm clean; rslib build",
    "clean": "rimraf ./.rslib ./dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contentful/optimization-api-schemas": "workspace:*",
    "contentful": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "catalog:",
    "@rslib/core": "catalog:",
    "@types/node": "catalog:",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  }
}
