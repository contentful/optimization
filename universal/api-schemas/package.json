{
  "name": "@contentful/optimization-api-schemas",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "pnpm clean; rslib build && build-tools emit-dual-dts ./dist",
    "clean": "rimraf ./.rslib ./dist .tsbuildinfo",
    "test:unit": "echo \"No unit tests necessary\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contentful/optimization-api-schemas": "workspace:*",
    "contentful": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "build-tools": "workspace:*",
    "@microsoft/api-extractor": "catalog:",
    "@rslib/core": "catalog:",
    "@types/node": "catalog:",
    "rimraf": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:"
  }
}
